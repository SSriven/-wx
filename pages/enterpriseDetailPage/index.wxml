    <view class="picker">
      <text class="key">成立时间</text>
      <i-icon class="unfold" type="unfold" />
      <text class="value">2018年11月14日</text>
    </view>
  </picker>
  <picker bindchange="bindPickerChange" disabled="{{disabled}}" range="{{['微型','小型','中型','大型']}}">
