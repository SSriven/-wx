<!--pages/enterpriseDetailPage/index.wxml-->
<text class="editor" bindtap="onEditor">编辑</text>
<i-panel title="企业信息">
  <picker bindchange="bindPickerChange" disabled="{{disabled}}" range="{{[1,2,3]}}">
    <view class="picker">
      <text class="key">社区(乡镇)</text>
      <i-icon class="unfold" type="unfold" />
      <text class="value">中心镇-社区B</text>
    </view>
  </picker>
  <i-input value="B企业C" title="企业名称" right disabled="{{disabled}}" mode="{{disabled ? 'normal' : 'wrapped'}}"/>
  <i-input value="12345" title="信用代码" type="number" right disabled="{{disabled}}" mode="{{disabled ? 'nomal' : 'wrapped'}}"/>
  <i-input value="第三方" title="通信地址" right disabled="{{disabled}}" mode="{{disabled ? 'normal' : 'wrapped'}}"/>
  <i-input value="33180" title="邮政编码" type="number" right disabled="{{disabled}}" mode="{{disabled ? 'normal' : 'wrapped'}}"/>
  <i-input value="115.7187" title="经度" type="number" right disabled="{{disabled}}" mode="{{disabled ? 'normal' : 'wrapped'}}"/>
  <i-input value="28.6048" title="纬度" type="number" right disabled="{{disabled}}" mode="{{disabled ? 'normal' : 'wrapped'}}"/>
